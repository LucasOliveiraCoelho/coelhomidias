{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "object-curly-newline": "off",
        "react/jsx-one-expression-per-line": "off",
        "react/jsx-no-constructed-context-values": "off",
        "react/jsx-wrap-multilines": "off"
    }
}
